{
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ $fileName -std=gnu++23 -Wall -Wextra -O2 -I $workspaceRoot/ac-library -o $fileNameWithoutExt.out && oj test -c ./$fileNameWithoutExt.out -d ./tests && rm ./$fileNameWithoutExt.out",
        "python": "cd $dir && oj test -c \"python3 $fileName\" -d ./tests"
    }
}